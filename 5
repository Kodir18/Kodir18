#включить <stdio.h>
		#включить <cmath>
		

		int main()
		{
		float x1 = 5; // Задаем координаты точек
		поплавок x2 = 7;
		поплавок y1 = 2;
		поплавок y2 = 9;
		длина поплавка;
		length = sqrt((x2 - x1)*(x2 - x1) + (y2 - y1)*(y2 - y1)); // По теореме Пифагора вычисляем расстояние между точками
		printf_s("%f", length);// Выводим длину
		возврат 0;
		}
